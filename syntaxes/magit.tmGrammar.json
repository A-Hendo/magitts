{
  "scopeName": "source.magit",
  "patterns": [
    {
      "include": "#expression"
    },
    {
      "include": "#diff"
    }
  ],
  "repository": {
    "expression": {
      "patterns": [
        {
          "include": "#sectionHeader"
        },
        {
          "include": "#branchName"
        }
      ]
    },
    "sectionHeader": {
      "match": "Untracked files|((Unstaged|Staged) changes)|Stashes|Recent commits",
      "name": "markup.bold keyword.section.header.magit",
      "comment": ";; kan bruke markup.bold.sectionHeader"
    },
    "branchName": {
      "match": "master",
      "name": "meta string support.master.magit"
    },
    "diff": {
      "begin": "(?=^@@)",
      "end": "^$",
      "patterns": [
        {
          "include": "source.diff"
        }
      ]
    }
  }
}